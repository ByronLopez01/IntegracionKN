<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Wave Release</title>
    @* Renderiza los estilos definidos en la página (EnviarCache.cshtml) *@
    @await RenderSectionAsync("Styles", required: false)
</head>
<body>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    @* Renderiza los scripts definidos en la página (EnviarCache.cshtml) *@
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>